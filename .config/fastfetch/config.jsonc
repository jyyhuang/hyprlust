{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
    "source": "~/.config/fastfetch/images/miku_circle_profile.png",
    "type": "kitty-direct",
    "width": 22,
    "height": 13,
    "padding": {
      "top": 5,
      "left": 2,
    },
    "preserveAspectRatio": true,
  },
  "display": {
