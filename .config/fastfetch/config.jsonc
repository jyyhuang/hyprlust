    },
    "break",
    {
      "type": "os",
      "key": " ",
      "keyColor": "34", // = color4
    },
    {
      "type": "kernel",
      "key": " ",
      "keyColor": "34",
    },
    {
      "type": "packages",
      "key": " ",
      "keyColor": "34",
    },
    {
