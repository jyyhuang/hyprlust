      "key": "󰓡 ",
      "keyColor": "34",
    },
    {
      "type": "disk",
      "key": "󰋊 ",
      "keyColor": "34",
    },
    "break",
    {
      "type": "colors",
    },
    "break",
    {
      "type": "custom",
      "format": "\u001b[90m  \u001b[31m  \u001b[32m  \u001b[33m  \u001b[34m  \u001b[35m  \u001b[36m  \u001b[37m  \u001b[91m  \u001b[92m  \u001b[93m",
